{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "esnext"],
    "module": "esnext",
    "moduleResolution": "node",
    "types": ["cypress", "node"],
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.d.ts",
    "../cypress.config.ts"
  ]
}
